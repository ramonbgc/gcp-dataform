config {
 type: "view",
 schema: "staging",
 description: "Cleaned version of stackoverflow.badges",
 tags: ["staging"]
}
select
 id as post_id,
 creation_date as created_at,
 'answer' as type,
 title,
 body,
 owner_user_id,
 cast(parent_id as string) as parent_id,
 tags
from
 ${ref("posts_answers")}
where creation_date>timestamp("2020-12-31")